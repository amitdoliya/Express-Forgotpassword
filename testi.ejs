<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.2/font/bootstrap-icons.css">
    <link rel="stylesheet" href="/admin/css/style.css">
</head>
<body>
    <%-include('../../partials/admin/header.ejs')%>
             <section >
                <div class="container">
                <div class="row">
                    <%-include('../../partials/admin/left.ejs')%>
                    <div class="col-md-9">
                        <h2>Testinominal Management</h2>
                        <table table class="table table-hover">
                            <tr>
                                <th>S.NO</th>
                                <th>Image </th>
                                <th>Quotes</th>
                                <th>Company Name</th>
                                 <th>Status</th> 
                                 <th>Delete</th>                       
                            </tr>
                                    <%record.forEach((result,key)=>{%>
                                    <tr>
                                    <td><%=key+1%></td>
                                    <td><img style="width:100px;"src="/upload/<%=result.img%>" alt=""></td>
                                    <td><%=result.testi%></td>
                                    <td><%=result.cname%></td>
                                   <%if(result.Status=='publish'){%>
                                    <td><a href="/admin/testistatus/<%=result.id%>"><button class="btn btn-success"><%=result.Status%></button></a></td>
                                    <%}else{%>
                                        <td><a href="/admin/testistatus/<%=result.id%>"><button class="btn btn-danger"><%=result.Status%></button></a></td>
                                    <%}%>
                                        <td><a href="/admin/testidelete/<%=result.id%>"><i class="bi bi-trash"style="color:red; font-size:20px;"></i></a></td> 
                                    
                                
                                   <%})%>
                                </tr>
                        </table>
                    </div>
                </div>
            </div>
            </section> 
<%-include('../../partials/admin/footer.ejs')%>
</body>
</html>