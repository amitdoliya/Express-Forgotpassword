<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.2/font/bootstrap-icons.css">
    <link rel="stylesheet" href="/admin/css/style.css">
</head>
<body>
    <%-include('../../partials/admin/header.ejs')%>
    
    </section>
             <section >
                <div class="container">
                <div class="row">
                    <%-include('../../partials/admin/left.ejs')%>
                   <div class="col-md-9">
                    <h3>Vechile Number Search</h3>
                    <form action="/admin/parkingsearch/:id" method="post">
                    <label for="">Enter Vechile Number</label>
                   <input type="search" placeholder="Searching anything" name="search" id="">
                    <button type="submit">Search</button>
                </form>
              
                        <h2>Parking Management</h2>
                       
                        <a href="/admin/parkingform"><button class="btn btn-danger form-control">Add new Entry Here</button></a>
                       
            
                        <table class="table table-hover">
                              <tr>
                                    <th>S.no.</th> 
                                    <th>Vichile Number</th>
                                    <th>Vichile type</th>
                                    <th>Vichile In Time</th>
                                    <th>Vichile Out Time</th>
                                    <th>Charged Amount</th>
                                    <th>Vichile status</th>
                                    <th>Print </th>
                                </tr>
                                <%record.forEach((result,key)=>{%>
                             <tr>
                                <td><%=key+1%></td>
                                <td><%=result.vno%></td>
                                <td><%=result.vtype%></td>
                                <td><%=result.vin%></td>
                                <td><%=result.vout%></td>
                                <td><%=result.amount%></td>
                                <%if(result.status=='IN'){%>
                              <td><a href="/admin/parkingupdaterecord/<%=result.id%>"><button><%=result.status%></button></a></td>
                               <%}else{%>
                                <td><a href="/admin/parkingupdaterecord/<%=result.id%>"><button disabled ><%=result.status%></button></a></td>
                              <%}%>
                              <%if(result.status=='IN'){%>
                              <td><a href="/admin/print/<%=result.id%>"><button disabled>Print</button></a></td>
                             <%}else{%>
                                <td><a href="/admin/print/<%=result.id%>"><button>Print</button></a></td>
                              <%}%>
                             </tr>
                            <%})%>
                                    
                        </table>
                 
                    </div>
                </div>
            </div>

            </section> 
<%-include('../../partials/admin/footer.ejs')%>
</body>
</html>